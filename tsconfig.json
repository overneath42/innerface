{
    "exclude": [
        "node_modules",
        "dist",
        "docs",
        "fixtures",
        ".vscode"
    ],
    "compilerOptions": {
        "lib": [
            "dom",
            "es6"
        ],
        "module": "commonjs",
        "noImplicitAny": false,
        "sourceMap": true,
        "strictNullChecks": false,
        "outDir": "out",
        "skipLibCheck": true,
        "target": "es5",
        "typeRoots": [
            "node_modules/@types"
        ],
        "types": [
            "jest",
            "lodash",
            "inflection"
        ]
    },
    "compileOnSave": false
}